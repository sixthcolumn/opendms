				<xs:annotation>
					<xs:documentation>Suffix to the street name. For example: North, South, East, West.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="suiteNumber" type="xs:string" minOccurs="0" sawsdl:modelReference="http://iec.ch/TC57/CIM-iec61970cim16v04_iec61968cim12v01_iec62325cim01v07#StreetDetail.suiteNumber">
				<xs:annotation>
					<xs:documentation>Number of the apartment or suite.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="xs:string" minOccurs="0" sawsdl:modelReference="http://iec.ch/TC57/CIM-iec61970cim16v04_iec61968cim12v01_iec62325cim01v07#StreetDetail.type">
				<xs:annotation>
					<xs:documentation>Type of street. Examples include: street, circle, boulevard, avenue, road, drive, etc.</xs:documentation>
				</xs:annotation>
