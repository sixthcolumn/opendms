			<xs:element name="withinTownLimits" type="xs:boolean" minOccurs="0" sawsdl:modelReference="http://iec.ch/TC57/CIM-iec61970cim16v04_iec61968cim12v01_iec62325cim01v07#StreetDetail.withinTownLimits">
