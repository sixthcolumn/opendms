					<xs:documentation>Energy service consumer.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="generatorOperator">
				<xs:annotation>
					<xs:documentation>Generator merchant operator.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="generatorOwner">
