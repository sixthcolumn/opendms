          <xs:sequence>
            <xs:element name="ActivePower" type="m:ActivePower" minOccurs="0"/>
            <xs:element name="ApparentPower" type="m:ApparentPower" minOccurs="0"/>
            <xs:element name="ReactivePower" type="m:ReactivePower" minOccurs="0"/>
