<xs:schema targetNamespace="http://www.epri.com/2013/DERGroupForecast#" elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://langdale.com.au/2005/Message#" xmlns:a="http://langdale.com.au/2005/Message#" xmlns:m="http://www.epri.com/2013/DERGroupForecast#" xmlns:sawsdl="http://www.w3.org/ns/sawsdl">
