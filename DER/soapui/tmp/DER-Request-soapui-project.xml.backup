    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="UnitSymbol">
    <xs:annotation>
      <xs:documentation>For DER the three primary unit symbols in use are W, VA, and VAr. This is for Active Power, Apparent Power, and Reactiev Power, respectively.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="W"/>
      <xs:enumeration value="VA"/>
      <xs:enumeration value="VAr"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="ActivePower">
    <xs:annotation>
      <xs:documentation>Expressed in Watts, with a symbol of W.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="multiplier" type="m:multiplier"/>
      <xs:element name="unit" type="xs:string" default="W"/>
      <xs:element name="value" type="xs:float"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ApparentPower">
    <xs:annotation>
      <xs:documentation>Expressed in Volt-Ampere, with a symbol of VA.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="multiplier" type="m:multiplier"/>
      <xs:element name="unit" type="xs:string" default="VA"/>
      <xs:element name="value" type="xs:float"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ReactivePower">
