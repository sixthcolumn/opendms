    <xs:sequence>
      <xs:element name="Header" type="msg:HeaderType"/>
      <xs:element name="Request" type="tns:GetDERGroupCapabilityRequestType"/>
      <xs:element name="Payload" type="tns:GetDERGroupCapabilityPayloadType" minOccurs="0"/>
