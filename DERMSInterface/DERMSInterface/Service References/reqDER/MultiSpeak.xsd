  <xs:complexType name="powerMonitors">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="unbounded" name="powerMonitor" type="msp:powerMonitor" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="clearanceInstanceIDs">
    <xs:annotation>
      <xs:documentation>List of clearance instances that are associated with this clearanceCertificate. ClearanceInstances may be referenced using just their clearanceInstanceID (objectID of the clearanceInstance.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="unbounded" name="clearanceInstanceID" type="com:objectID">
        <xs:annotation>
          <xs:documentation>This is a pointer to a clearanceInstance that is included as part of the containing clearanceCertificate.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="clearanceTags">
    <xs:annotation>
      <xs:documentation>Tags associated with a switching order.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="unbounded" name="clearanceTag" type="msp:clearanceTag">
        <xs:annotation>
          <xs:documentation>A tag associated with this clearanceCertificate.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="clearanceTag">
    <xs:annotation>
      <xs:documentation>This is a tag associated with the containing clearanceCertificate.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="msp:mspObject">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" name="tagType" type="enum:tagType">
            <xs:annotation>
              <xs:documentation>The kind of clearanceTag.  Enumerated values include: HoldOpen - this means that the device shall be maintained in the open position while the tag is in effect; HoldClosed - this means that the device shall be maintained in the closed position while the tag is in effect; Full - this means that no action is permitted to be taken on this device and no reporting is to be made; Information - this tag type is attached to include information on the device state, but that no inhibition is placed on the operation or reporting on the device; Control - this means that no control action is to be taken on the device, but reporting may be performed; Alarm - this means that alarming is to be suppressed while the tag is in effect; Scan - this means that scanning is to be inhibited while the tag is in effect; Other - system will provide a vendor-specific tagKind.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" maxOccurs="1" name="tagName" type="xs:string" />
          <xs:element minOccurs="1" maxOccurs="1" name="taggedDeviceRef" type="com:objectRef">
            <xs:annotation>
              <xs:documentation>A reference to the device that is to be controlled in response to this tag.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="tagPlacedDateTime" type="prim:mspDateTime">
            <xs:annotation>
              <xs:documentation>Date/time that the tag was placed.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="tagReason" type="xs:string">
            <xs:annotation>
              <xs:documentation>Reason that the tag was placed.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="tagLocation" type="xs:string">
            <xs:annotation>
              <xs:documentation>Physical location of the tag placement.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="clearanceRef" type="com:objectRef">
            <xs:annotation>
              <xs:documentation>Reference to either the clearanceCertificate or the clearanceInstance of which this tag is a component.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="certificateAuthorizations">
    <xs:annotation>
      <xs:documentation>Information about authorization events related to this clearanceCertificate.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="certificateAuthorization" type="msp:authorization" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="authorization">
    <xs:complexContent mixed="false">
      <xs:extension base="com:mspExtensible">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" name="authorizationType" type="enum:authorizationType">
            <xs:annotation>
              <xs:documentation>This element identifies the type of authorization.  Examples include: RequestedBy, CreatedBy, CheckedBy, ModifiedBy, ReleasedBy, and Other.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" maxOccurs="1" name="authorizedBy" type="xs:string">
            <xs:annotation>
              <xs:documentation>This is the name or identifier for the person who performed the action described by the authorizationType element, such as "ReleasedBy".</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" maxOccurs="1" name="authorizationDateTime" type="prim:mspDateTime">
            <xs:annotation>
              <xs:documentation>The date and time that the authorization event occurred that is described by the authorizationType element.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="externalRequest">
    <xs:annotation>
      <xs:documentation>Documentation of the source of a request for a clearanceCertificate that originated from an external organization.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="com:mspExtensible">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="organization" type="xs:string">
            <xs:annotation>
              <xs:documentation>Another organization or person that has requested the clearanceCertificate owner to issue the clearanceCertificate.  This element might contain a pointer (objectID) to an organization object (an organizationID) or might contain just a string name or identifier for the organization if the owner does not maintain organization records.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="dateOfRequest" type="prim:mspDateTime">
            <xs:annotation>
              <xs:documentation>The date/time that the clearanceCertificate was requested.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="requestingParty" type="xs:string">
            <xs:annotation>
              <xs:documentation>Name or identifier of the person who requested the clearanceCertificate.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="switchingStepWork">
    <xs:annotation>
      <xs:documentation>This is the work that needs to be performed in order to accomplish a manual switching step.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="msp:mspWork">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" name="operation" type="enum:switchingStepOperationType">
            <xs:annotation>
              <xs:documentation>Operation to be taken on this device. Sample operations are Open, Close, Tag open, Tag close, Set tags, Install ground, Remove ground, Check open, Check closed, etc. </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="deviceDescription" type="xs:string" />
          <xs:element minOccurs="1" maxOccurs="1" name="deviceRef" type="com:objectRef">
            <xs:annotation>
              <xs:documentation>Device to be switched. For this purpose, locationReferences shall not be used to specify the device that is to be operated in this switchingStep because only one device can be specified during one step.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" maxOccurs="1" name="desiredEndState" type="enum:switchingEndStateType">
            <xs:annotation>
              <xs:documentation>The desired end state for this device after this step is completed. Sample enumerations include Opened, Closed, Tagged open, Tagged closed, Grounded, Ungrounded, In service, etc.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="instructedBy" type="xs:string" />
          <xs:element minOccurs="0" maxOccurs="1" name="instructedDateTime" type="prim:mspDateTime" />
          <xs:element minOccurs="0" maxOccurs="1" name="completedBy" type="xs:string" />
          <xs:element minOccurs="0" maxOccurs="1" name="completedDateTime" type="prim:mspDateTime" />
          <xs:element minOccurs="0" maxOccurs="1" name="stepStatus" type="enum:switchingStepStatusType">
            <xs:annotation>
              <xs:documentation>The status of this switching step.  Possible enumerations include: Proposed, Instructed, Confirmed, Aborted, Skipped, Other. The supported values of stepStatus should be discovered by a call to GetDomainMembers on the application that is the system of record for switching orders.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="workHistoryEvents">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="unbounded" name="workHistoryEvent" type="msp:workHistoryEvent">
