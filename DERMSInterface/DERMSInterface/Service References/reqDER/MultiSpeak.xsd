            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="documentation" type="xs:string">
            <xs:annotation>
              <xs:documentation>This is additional information about this work priority.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
