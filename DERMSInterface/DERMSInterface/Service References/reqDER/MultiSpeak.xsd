      <xs:element minOccurs="1" maxOccurs="unbounded" name="pricingPeriod" type="msp:pricingPeriod" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="pricingPeriod">
    <xs:complexContent mixed="false">
      <xs:extension base="com:mspExtensible">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" name="charges" type="msp:charges">
            <xs:annotation>
              <xs:documentation>Cost per unit for commodities during this pricing period.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" maxOccurs="1" name="timePeriod" type="com:timePeriod" />
