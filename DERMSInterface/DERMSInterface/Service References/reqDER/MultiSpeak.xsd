              <xs:documentation>List of other utility services associated with this serviceLocation.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="serviceOrderIDs" type="msp:serviceOrderIDs">
            <xs:annotation>
