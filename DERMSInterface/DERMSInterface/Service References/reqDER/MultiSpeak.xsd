    <xs:annotation>
      <xs:documentation>Contains the outage detection events that have been processed by the Outage Management System for a specific service location and outage.  The objectID is the unique outageDetectionRecordID for all outage detection events at a given service location.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="msp:mspObject">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="phaseCode" type="enum:phaseCode">
            <xs:annotation>
              <xs:documentation>Phasing code.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="outageLocation" type="msp:outageLocation" />
          <xs:element minOccurs="0" maxOccurs="1" name="outageCustomer" type="msp:outageCustomer" />
          <xs:element minOccurs="0" maxOccurs="1" name="messages" type="msp:messages" />
          <xs:element minOccurs="0" maxOccurs="1" name="loggedOutageDetectionEvents" type="msp:loggedOutageDetectionEvents" />
          <xs:element minOccurs="0" maxOccurs="1" name="priority" type="enum:priorityType">
            <xs:annotation>
              <xs:documentation>Designation of importance level of this event.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="resolvedLevel" type="enum:resolvedLevelKind">
            <xs:annotation>
              <xs:documentation>Indicates if this call was resolved to a specific meter</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="timeTroubleBegan" type="prim:mspDateTime">
            <xs:annotation>
              <xs:documentation>The approximate time the trouble began or was reported.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="timeOfInitialEvent" type="prim:mspDateTime">
            <xs:annotation>
              <xs:documentation>The initial event time. This is the initial time the event was logged. </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="timeOfLastEvent" type="prim:mspDateTime">
            <xs:annotation>
              <xs:documentation>Time last event was logged.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="eventsLogged" type="xs:long">
            <xs:annotation>
              <xs:documentation>Number of events logged.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
