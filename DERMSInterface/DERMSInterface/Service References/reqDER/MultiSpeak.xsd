    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="com:mspExtensible">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" name="constituentUnitRef" type="com:objectRef" />
          <xs:element minOccurs="1" maxOccurs="1" name="quantity" type="xs:float" />
          <xs:element minOccurs="1" maxOccurs="1" name="applicationActions" type="enum:unitAction">
            <xs:annotation>
              <xs:documentation>This element describes the action that is being taken on this contituent unit.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="statusOfUnit" type="enum:statusOfUnit" />
          <xs:element minOccurs="0" maxOccurs="1" name="variant" type="msp:variant" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="endDeviceEvents">
