  <xs:complexType name="outageStatus">
    <xs:annotation>
      <xs:documentation>This class is a summary of the current status of an outage.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="com:mspExtensible">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" name="outageState" type="enum:outageState" />
          <xs:element minOccurs="0" maxOccurs="1" name="crewState" type="enum:resourceState">
            <xs:annotation>
              <xs:documentation>This is the state of the resource (crew) that has primary responsibility for this outage.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="firstETA" type="prim:mspDateTime">
            <xs:annotation>
              <xs:documentation>The time that the first resource (crew) is expected to reach the site of the outage.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="ETOR" type="prim:mspDateTime">
            <xs:annotation>
              <xs:documentation>Estimated time of restoration of the outage.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="capacitor">
    <xs:annotation>
      <xs:documentation>An individual capacitor unit.  Such a unit might be tracked individually as an asset, if so, the asset information SHOULD be populated.  If the details of the individual capacitor units is not important for a specific interface, the asset information SHOULD be omitted.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="msp:mspAsset">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="unitRating" type="com:reactivePower">
            <xs:annotation>
              <xs:documentation>The rating in reactive power units for this capacitor unit.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="unitRatedVoltage" type="com:voltage" />
          <xs:element minOccurs="0" maxOccurs="1" name="BIL" type="com:voltage">
            <xs:annotation>
              <xs:documentation>Basic Impulse Level (BIL), a measure of the maximum pak voltage that this unit can withstand without  flashing over.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="ratedFrequency" type="com:frequency" />
          <xs:element minOccurs="0" maxOccurs="1" name="numberOfBushings" type="xs:integer">
            <xs:annotation>
              <xs:documentation>The number of insulation bushings that this capacitor unit has ( 1 or 2).  If the capacitor unit has one bushing it can only be connected phase to neutral; if it has two bushings it can be connected either phase to neutral or phase to phase.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="materialManagementAssemblyID" type="com:objectID">
            <xs:annotation>
              <xs:documentation>Pointer to the materialManagementAssembly (construction unit definition) of which this capacitor is an instance.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="capacitorBankID" type="com:objectID">
            <xs:annotation>
              <xs:documentation>Pointer to the capacitorBank, if any, that contains this capacitor unit.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CPRAdjustments">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="unbounded" name="CPRAdjustment" type="msp:CPRAdjustment" />
