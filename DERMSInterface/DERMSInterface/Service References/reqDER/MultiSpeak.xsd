            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="noteSubtype" type="xs:string">
            <xs:annotation>
              <xs:documentation>Additional classification information for this supplemental note if necessary.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" maxOccurs="1" name="noteValue" type="xs:string">
            <xs:annotation>
              <xs:documentation>This is the content of this note.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="outageRemark">
