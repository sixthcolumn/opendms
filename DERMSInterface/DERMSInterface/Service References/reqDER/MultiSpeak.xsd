          <xs:element minOccurs="0" maxOccurs="1" name="PANInterfaceIDs" type="msp:PANInterfaceIDs" />
          <xs:element minOccurs="0" maxOccurs="1" name="PANDeviceRefs" type="msp:PANDeviceRefs" />
          <xs:element minOccurs="0" maxOccurs="1" name="aggregatedGroups" type="msp:aggregatedGroups" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="stationAssemblyDetails">
    <xs:complexContent mixed="false">
      <xs:extension base="com:mspExtensible">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="CPRAdjustmentList" type="msp:CPRAdjustmentList" />
          <xs:element minOccurs="0" maxOccurs="1" name="pickList" type="msp:pickList" />
          <xs:element minOccurs="0" maxOccurs="1" name="laborItemList" type="msp:laborItemList" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="laborItems">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="unbounded" name="laborItem" type="msp:laborItem" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="laborItem">
    <xs:complexContent mixed="false">
      <xs:extension base="com:mspExtensible">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" name="laborCategoryID" type="com:objectID" />
          <xs:element minOccurs="1" maxOccurs="1" name="quantity" type="xs:float" />
          <xs:element minOccurs="0" maxOccurs="1" name="description" type="xs:string" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
