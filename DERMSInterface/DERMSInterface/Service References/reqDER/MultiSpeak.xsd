              <xs:documentation>The time at which the OMS split the outage.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" maxOccurs="1" name="originalOutage" type="msp:originalOutage" />
          <xs:element minOccurs="1" maxOccurs="1" name="splitReason" type="xs:string" />
          <xs:element minOccurs="1" maxOccurs="1" name="resultingOutages" type="msp:resultingOutages" />
