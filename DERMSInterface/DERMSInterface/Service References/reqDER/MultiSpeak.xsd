          <xs:element minOccurs="0" maxOccurs="1" name="attachments" type="msp:attachments" />
          <xs:element minOccurs="0" maxOccurs="1" name="GPSLocation" type="com:GPSLocation">
            <xs:annotation>
              <xs:documentation>A geographical location as expressed in a geographic coordinate system. The GPSLocation SHALL be expressed using the WGS84 datum. This object SHOULD be used for expressing all types of GPS data.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="CPRs" type="msp:CPRs" />
          <xs:element minOccurs="0" maxOccurs="1" name="assemblySource" type="msp:assemblySource" />
          <xs:element minOccurs="0" maxOccurs="1" name="spatialFeatures" type="msp:spatialFeatures">
            <xs:annotation>
              <xs:documentation>If BOTH the assembly and the associated spatialFeature are populated on this station, the assetLocation.locationReference of the spatialFeature SHALL be populated with a reference to the assembly with which the feature is associated.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
