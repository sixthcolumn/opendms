          <xs:element minOccurs="0" maxOccurs="1" name="dueDate" type="prim:mspDateTime">
            <xs:annotation>
              <xs:documentation>The date that the receivable is (or was) due.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="serviceLocationID" type="com:objectID" />
          <xs:element minOccurs="0" maxOccurs="1" name="serviceType" type="enum:serviceType">
            <xs:annotation>
              <xs:documentation>Type of utility service(e.g., Electric, Gas, Water, etc.).</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="accumulatedValue">
    <xs:annotation>
      <xs:documentation>This object implements an accumulator, such as that in a SCADA system.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="msp:mspObject">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" name="countedValue" type="xs:integer">
            <xs:annotation>
              <xs:documentation>Accumulated value being reported.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" maxOccurs="1" name="timeStamp" type="prim:mspDateTime">
            <xs:annotation>
              <xs:documentation>Time accumulated value was recorded.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="ceilingValue" type="xs:integer">
            <xs:annotation>
              <xs:documentation>This is the value beyond which the accumulator will roll over or latch at maximum.  </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="latchesAtMaximum" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>This is true for an accumulator that does not rollover upon reaching ceiling value.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="measurementID" type="com:objectID">
            <xs:annotation>
              <xs:documentation>This is a pointer to an optional set of measurementTypes that are carried in a catalog of MeasurementType objects. </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="recurringPaymentConfiguration">
    <xs:complexContent mixed="false">
      <xs:extension base="msp:mspObject">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" name="accountID" type="com:objectID">
            <xs:annotation>
              <xs:documentation>A reference to the customer account on which the recurring payment is to be made.  </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="serviceType" type="enum:serviceType">
            <xs:annotation>
              <xs:documentation>The type of utility service on which the payment is to be made.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" maxOccurs="1" name="configurationAction" type="msp:configurationAction">
            <xs:annotation>
              <xs:documentation>The configuration action (authorization, modification or cancellation).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="paymentMode" type="msp:paymentMode">
            <xs:annotation>
              <xs:documentation>The means by which payment will be made.  This element SHALL be included for authorizing or modifying a recurrentPaymentConfiguration, but MUST NOT be provided for the cancellation condition.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
