              <xs:documentation>Accounting code for this continuing property record.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" maxOccurs="1" name="quantity" type="xs:float">
            <xs:annotation>
              <xs:documentation>Amount of the materialManagementAssembly that should be allocated to this CPR.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="variant" type="msp:variant" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CPRAllocations">
