        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" name="receivableAmount" type="com:money">
            <xs:annotation>
              <xs:documentation>Amount of money that is due for this receivable, expressed in default currency unless the currencyCode attribute is set to a different currency type.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" maxOccurs="1" name="receivableType" type="msp:receivableType" />
