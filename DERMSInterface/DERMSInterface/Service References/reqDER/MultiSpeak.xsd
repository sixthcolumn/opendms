          <xs:element minOccurs="0" maxOccurs="1" name="CPPScheduleVersion" type="xs:string" />
          <xs:element minOccurs="1" maxOccurs="1" name="effectiveDate" type="msp:effectiveDate" />
          <xs:element minOccurs="1" maxOccurs="1" name="daysOfWeek" type="msp:daysOfWeek" />
