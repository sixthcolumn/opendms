  </xs:complexType>
  <xs:complexType name="lineEnvironmentalAttributesEntries">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="unbounded" name="lineEnvironmentalAttributes" type="msp:lineEnvironmentalAttributes" />
    </xs:sequence>
