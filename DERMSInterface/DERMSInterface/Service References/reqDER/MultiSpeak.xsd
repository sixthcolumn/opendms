          <xs:element minOccurs="0" maxOccurs="1" name="assetData" type="msp:assetData">
            <xs:annotation>
              <xs:documentation>If this class models a tracked asset, this optional element may be used to carry asset information.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="materialManagementAssemblyID" type="com:objectID">
            <xs:annotation>
              <xs:documentation>Pointer to the materialManagementAssembly (construction unit definition) of which this capacitorBankSwitch is an instance.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="capacitorBankSwitches">
