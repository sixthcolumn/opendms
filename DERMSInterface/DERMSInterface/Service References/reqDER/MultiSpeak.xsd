      <xs:documentation>A supplementalNote is a means to provide self-describing supplemental information about another class. </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="com:mspExtensible">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" name="noteType" type="xs:string">
