  <xs:complexType name="tariff">
    <xs:annotation>
      <xs:documentation>This is a utility tariff for service.  It is intended to model a rate tariff for utility service.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="msp:mspObject">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" name="revenueClass" type="xs:string">
            <xs:annotation>
              <xs:documentation>This is the type of revenue class, examples are Residential, Small Commercial, Large Commercial, Industrial, Irrigation, Lighting, Residential TOU, etc.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="purpose" type="xs:string">
            <xs:annotation>
              <xs:documentation>A description of the purpose for this tariff. </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" maxOccurs="1" name="rateCode" type="xs:string">
            <xs:annotation>
              <xs:documentation>This is a human-readable tariff identifier and is used as a pointer to this tariff in other objects.  This SHALL be the primaryIdentifier for this tariff instance.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="effectivePeriod" type="com:timePeriod">
            <xs:annotation>
              <xs:documentation>The effective period for this tariff.  The tariff becomes effective at the startTime and effectiveness ends at the endTime.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="tariffDescription" type="xs:string">
            <xs:annotation>
              <xs:documentation>A detailed description of the tariff.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="serviceCharacteristics" type="msp:serviceCharacteristics" />
          <xs:element minOccurs="0" maxOccurs="1" name="flatCharges" type="msp:charges">
            <xs:annotation>
              <xs:documentation>This tariff component models any flat charge, whether a service charge or a flat rate for a commodity.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="variablePricing" type="msp:rateComponent">
            <xs:annotation>
              <xs:documentation>This rate covers the real time pricing situation.  The price is established in real time.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="consumptionTiers" type="msp:consumptionTiers" />
          <xs:element minOccurs="0" maxOccurs="1" name="demandCharges" type="msp:demandCharges" />
          <xs:element minOccurs="0" maxOccurs="1" name="CPPSchedules" type="msp:CPPSchedules" />
          <xs:element minOccurs="0" maxOccurs="1" name="TOUSchedules" type="msp:TOUSchedules" />
        </xs:sequence>
