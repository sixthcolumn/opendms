            <xs:annotation>
              <xs:documentation>The type of equipment that includes this equipmentItem.  The allowable values for equipmentItem.equipmentType SHALL be discoverable from the system of record for equipment using the GetDomainMembers method.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="isCompanyOwned" type="xs:boolean" />
          <xs:element minOccurs="0" maxOccurs="1" name="LTDeviceRef" type="msp:locationTrackingDeviceRef">
