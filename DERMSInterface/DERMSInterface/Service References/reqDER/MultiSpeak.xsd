  <xs:complexType name="synchronousMachine">
    <xs:annotation>
      <xs:documentation>A synchronous machine that can operate as a synchronous motor, generator or condensor.  This object replaces the Version 3.0 generator object.  </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="msp:mspMotorGenerator">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="ssImpedanceID" type="com:equipmentCatalogRef">
            <xs:annotation>
              <xs:documentation>Description of steady-state device type.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="tranImpedanceID" type="com:equipmentCatalogRef">
            <xs:annotation>
              <xs:documentation>Description of transient device type.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="stImpedanceID" type="com:equipmentCatalogRef">
            <xs:annotation>
              <xs:documentation>Description of subtransient device type.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="model" type="xs:integer">
            <xs:annotation>
              <xs:documentation>Generator model code:

0 = Negative load.
1 = Swing generator (unlimited)
2 = Swing kVA
3 = Swing kVAR</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="type" type="cpsm:SynchronousMachineType">
            <xs:annotation>
              <xs:documentation>Modes that this synchronous machine can operate in, i.e., as a generator, condenser, or both.  This element is required for the CPSM profile.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="operatingMode" type="cpsm:SynchronousMachineOperatingMode">
            <xs:annotation>
              <xs:documentation>Current mode of operation, i.e., generator or condenser.  This element is required for the CPSM profile.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="holdVoltsZ" type="com:complexVoltage">
            <xs:annotation>
              <xs:documentation>For Swing kVAR model, voltage to hold.  Expressed in per unit.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="Pout" type="com:realPower">
            <xs:annotation>
              <xs:documentation>For Swing kVAR model, real power output.  </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="maxLeadQ" type="com:reactivePower">
            <xs:annotation>
              <xs:documentation>For Swing kVAR model, Maximum leading reactive power output.  </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="maxLagQ" type="com:reactivePower">
            <xs:annotation>
              <xs:documentation>For Swing kVAR model, Maximum lagging reactive power output.  </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="connection" type="enum:loadConnection">
            <xs:annotation>
              <xs:documentation>Load connection. 
(W for wye;  D for delta).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="maximumVoltage" type="com:voltage">
            <xs:annotation>
              <xs:documentation>Maximum kV limit for the unit.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="minimumVoltage" type="com:voltage">
            <xs:annotation>
              <xs:documentation>Minimum kV  limit for the unit.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="baseQ" type="com:reactivePower">
            <xs:annotation>
              <xs:documentation>Default base MVAr value. This value represents the initial MVAr that can be used by any application function.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="maxQ" type="com:reactivePower">
            <xs:annotation>
              <xs:documentation>Maximum MVAr limit. This is the maximum (nameplate) limit for the unit. This element is required for the CPSM profile.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="minQ" type="com:reactivePower">
            <xs:annotation>
              <xs:documentation>Minimum MVAr limit for the unit.  This element is required for the CPSM profile.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="ratedMVA" type="com:apparentPower">
            <xs:annotation>
              <xs:documentation>Nameplate MVA rating for the unit</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="generatingUnitID" type="com:objectID">
            <xs:annotation>
              <xs:documentation>This is a pointer to the GeneratingUnit, HydroGeneratingUnit or ThermalGeneratingUnit that contains this generator. Used in CPSM profile.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="regulatingControlID" type="com:objectID">
            <xs:annotation>
              <xs:documentation>This is a pointer to a RegulatingControl. Used in CPSM profile.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="initialReactiveCapabilityCurve" type="cpsm:ReactiveCapabilityCurve">
            <xs:annotation>
              <xs:documentation>A ReactiveCapabilityCurve is not necessary for compliance with the CPSM profile, and is not typically used in MultiSpeak models.  If the capability curve is not used, then the minQ and maxQ values will be used.  If the ReactiveCapabilityCurve is provided, then the minQ and maxQ values are not required.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="impedances" type="msp:synchronousMachineImpedances">
            <xs:annotation>
              <xs:documentation>These values are used only in CIM models, they are not required for MultiSpeak models.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="synchronousMachineEntries">
