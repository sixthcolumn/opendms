          <xs:element minOccurs="0" maxOccurs="1" name="switchOnValue" type="msp:switchOnValue" />
          <xs:element minOccurs="0" maxOccurs="1" name="switchOffValue" type="msp:switchOffValue" />
          <xs:element minOccurs="0" maxOccurs="1" name="switchTimeDelay" type="com:duration" />
          <xs:element minOccurs="0" maxOccurs="1" name="controlCircuitRef" type="com:objectRef">
            <xs:annotation>
