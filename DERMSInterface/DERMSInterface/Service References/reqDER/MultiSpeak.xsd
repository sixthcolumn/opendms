  <xs:complexType name="regulatorEntry">
    <xs:annotation>
      <xs:documentation>This is an electrical equipment catalog entry for a regulator.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="msp:mspElectricalDefinition">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="ratedVoltage" type="com:voltage">
            <xs:annotation>
              <xs:documentation>The rated voltage of the regulator.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="ratedPower" type="com:apparentPower">
            <xs:annotation>
              <xs:documentation>The rated power for this regulator.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="ampacity" type="com:current">
            <xs:annotation>
              <xs:documentation>The rated ampacity for this regulator.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="BIL" type="com:voltage">
            <xs:annotation>
              <xs:documentation>Basic Insulation Level (BIL) for switchgear, insulators, etc. A reference insulation level expressed as the impulse crest voltage of a nominal wave, typically 1.2 X 50 microsecond.  This is a measure of the ability of the insulation to withstand very high voltage surges.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="numberOfSteps" type="xs:integer">
            <xs:annotation>
              <xs:documentation>Number of steps of regulation that this regulator supports.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="stepVoltageIncrement" type="prim:percent">
            <xs:annotation>
              <xs:documentation>Tap step increment, in per cent of nominal voltage, per step position.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="PTRatio" type="xs:double">
            <xs:annotation>
              <xs:documentation>Ratio of the potential transformer in this unit.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="CTRating" type="com:current">
            <xs:annotation>
              <xs:documentation>Rating of the current transformer, expressed in amperes.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="windingType" type="enum:regulatorWindingType">
            <xs:annotation>
              <xs:documentation>The regulator winding type (A,B).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="frequency" type="com:frequency">
            <xs:annotation>
              <xs:documentation>Frequency at which stated device ratings apply, typically 50 Hz or 60 Hz.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
