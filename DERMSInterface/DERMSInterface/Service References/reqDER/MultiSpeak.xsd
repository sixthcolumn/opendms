              <xs:documentation>A pointer to the customer that is the subject of this agreement.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="accountID" type="com:objectID">
            <xs:annotation>
              <xs:documentation>A pointer to the account that is the subject of this agreement.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="serviceLocationID" type="com:objectID">
            <xs:annotation>
              <xs:documentation>A pointer to the serviceLocation that is the subject of this agreement.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="servicePointID" type="com:servicePointID">
            <xs:annotation>
              <xs:documentation>A pointer to the service point that is the subject of this agreement.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="revenueClass" type="xs:string">
