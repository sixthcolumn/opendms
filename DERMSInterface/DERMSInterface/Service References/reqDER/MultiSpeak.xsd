          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="reverseFlowR" type="com:voltage">
            <xs:annotation>
              <xs:documentation>Resistive line drop compensation resistance for current in reverse flow direction, expressed in volts on the PT secondary base..</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="reverseFlowX" type="com:voltage">
            <xs:annotation>
              <xs:documentation>Reactive line drop compensation reactance for current in reverse flow direction,expressed in volts on the PT secondary base.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="materialManagementAssemblyID" type="com:objectID">
            <xs:annotation>
              <xs:documentation>Pointer to the materialManagementAssembly (construction unit definition) of which this regulator is an instance.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="regulatorBankID" type="com:objectID">
