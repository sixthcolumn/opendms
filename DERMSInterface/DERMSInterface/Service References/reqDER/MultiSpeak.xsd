              <xs:documentation>This is the meter identifier of the meter associated with this outage.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="serviceLocationID" type="com:objectID">
            <xs:annotation>
              <xs:documentation>This is the ID of the serviceLocation with which this outage event is associated.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="servicePointID" type="com:servicePointID">
            <xs:annotation>
              <xs:documentation>This is the object identifier for the service point associated with this outage event.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="accountID" type="com:objectID">
            <xs:annotation>
              <xs:documentation>Customer account number.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="timeOfInterruption" type="prim:mspDateTime" />
          <xs:element minOccurs="0" maxOccurs="1" name="timeRestored" type="prim:mspDateTime" />
          <xs:element minOccurs="0" maxOccurs="1" name="interruptionDuration" type="xs:integer">
            <xs:annotation>
              <xs:documentation>This is the interruption durration in minutes.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="customerResponsible" type="xs:boolean">
