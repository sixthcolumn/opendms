<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:gml="http://www.multispeak.org/V5.0/gml" xmlns:com="http://www.multispeak.org/V5.0/commonTypes" xmlns:enum="http://www.multispeak.org/V5.0/enumerations" xmlns:msp="http://www.multispeak.org/V5.0" xmlns:sandbox="http://www.multispeak.org/V5.0/sandbox" xmlns:prim="http://www.multispeak.org/V5.0/primitives" xmlns="http://www.multispeak.org/V5.0/sandbox" targetNamespace="http://www.multispeak.org/V5.0/sandbox" version="5.0.4" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import schemaLocation="mspEnumerations.xsd" namespace="http://www.multispeak.org/V5.0/enumerations" />
  <xs:import schemaLocation="mspCommonTypes.xsd" namespace="http://www.multispeak.org/V5.0/commonTypes" />
  <xs:import schemaLocation="mspPrimitives.xsd" namespace="http://www.multispeak.org/V5.0/primitives" />
  <xs:import schemaLocation="MultiSpeak.xsd" namespace="http://www.multispeak.org/V5.0" />
  <xs:complexType name="asFound">
    <xs:complexContent mixed="false">
      <xs:extension base="sandbox:meterConditions" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="asLeft">
    <xs:annotation>
      <xs:documentation>Conditions as meter left after test.
			</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="sandbox:meterConditions" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="damageAssessment">
    <xs:annotation>
      <xs:documentation>This is a field damage assessment report.
			</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="msp:mspObject">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="estimated" type="xs:float">
            <xs:annotation>
              <xs:documentation>Estimated time to repair in hours
							</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="eventID" type="xs:string">
            <xs:annotation>
              <xs:documentation>Event ID if part of an event or outage
							</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="jobNumber" type="xs:string">
            <xs:annotation>
              <xs:documentation>The jobNumber associated with this assessment.
							</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="expiresOn" type="prim:mspDateTime" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="damageAssessments">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="unbounded" name="damageAssessment" type="sandbox:damageAssessment" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="damageAssessmentLocation">
    <xs:complexContent mixed="false">
      <xs:extension base="msp:mspPointObject">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="address" type="com:address" />
          <xs:element minOccurs="0" maxOccurs="1" name="locationOffset" type="xs:string">
            <xs:annotation>
              <xs:documentation>
								Allows the collector to specify that the location is a offset of the
								collected location. Examples. 5 spans north, 2 houses east, or 1
								tap north.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="damageAssessments" type="sandbox:damageAssessments" />
          <xs:element minOccurs="0" maxOccurs="1" name="attachments" type="msp:attachments" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="blinkAlarm">
    <xs:complexContent mixed="false">
      <xs:extension base="sandbox:mspAlarm">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="countedValue" type="xs:integer">
            <xs:annotation>
              <xs:documentation>Accumulated value being reported.
							</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="ceilingValue" type="xs:integer">
            <xs:annotation>
              <xs:documentation>This is the value beyond which the accumulator
								will roll over or latch at maximum.  </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="latchesAtMaximum" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>This is true for an accumulator that does not
								rollover upon reaching ceiling value.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="meterTests">
    <xs:complexContent mixed="false">
      <xs:extension base="msp:mspReferable">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="electricMeterTests" type="sandbox:electricMeterTests">
            <xs:annotation>
              <xs:documentation>This is an array of tests on electricMeters.
							</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="generatingUnits">
    <xs:annotation>
      <xs:documentation>The generating units associated with this DER.
			</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="generatingUnitRef" type="com:objectRef" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="materialRequisitionCancellation">
    <xs:annotation>
      <xs:documentation>This is the payload of the
				CancelMaterialRequisition method.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="msp:mspReferable">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" name="requisitionID" type="com:objectID">
            <xs:annotation>
              <xs:documentation>The objectID of the material requisition that
								is to be canceled.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" maxOccurs="1" name="workItemReference" type="com:objectRef">
            <xs:annotation>
              <xs:documentation>This is a pointer to the work item to which
								this material was to be committed. The work item may be a
								workOrder, a design, a serviceOrder or any other item of work.
							</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="designReference" type="com:nounUnspecifiedRef">
            <xs:annotation>
              <xs:documentation>If the workItemReference is a workOrder, this
								is a reference to the specific design on that workOrder to which
								the material was to have been committed.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="employeeTimeRecord">
    <xs:complexContent mixed="false">
      <xs:extension base="msp:mspObject">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="employeeID" type="com:objectID">
            <xs:annotation>
              <xs:documentation>Unique identifier for an employee.
							</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="employeeName" type="xs:string" />
          <xs:element minOccurs="0" maxOccurs="1" name="hoursWorked" type="xs:float">
            <xs:annotation>
              <xs:documentation>Total hours worked by the employee during this
								time period.(Exactly 1 per employee)|.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="employeeTimeRecordList">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="unbounded" name="employeeTimeRecord" type="sandbox:employeeTimeRecord">
        <xs:annotation>
          <xs:documentation>A list of all employee time records.
					</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="endDeviceShipment">
    <xs:complexContent mixed="false">
      <xs:extension base="msp:mspObject">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="utility" type="xs:string" />
          <xs:element minOccurs="0" maxOccurs="1" name="manufacturer" type="xs:string" />
          <xs:element minOccurs="0" maxOccurs="1" name="poReferenceNumber" type="xs:string">
            <xs:annotation>
              <xs:documentation>Purchase order number.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="shippingTicketNumber" type="xs:string" />
          <xs:element minOccurs="0" maxOccurs="1" name="recipient" type="xs:string">
            <xs:annotation>
              <xs:documentation>Party to whom the shipment is sent.
							</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="shippedDate" type="prim:mspDateTime">
            <xs:annotation>
              <xs:documentation>Date this shipment was sent.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="shippedToAddress" type="com:address" />
          <xs:element minOccurs="0" maxOccurs="1" name="receivedDate" type="prim:mspDateTime">
            <xs:annotation>
              <xs:documentation>Date this shipment was received.
							</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="receivedGasMeters" type="msp:receivedGasMeters" />
          <xs:element minOccurs="0" maxOccurs="1" name="receivedElectricMeters" type="msp:receivedElectricMeters" />
          <xs:element minOccurs="0" maxOccurs="1" name="receivedLoadManagementDevices" type="msp:receivedLoadManagementDevices" />
          <xs:element minOccurs="0" maxOccurs="1" name="receivedCDDevices" type="msp:receivedCDDevices" />
          <xs:element minOccurs="0" maxOccurs="1" name="receivedWaterMeters" type="msp:receivedWaterMeters" />
          <xs:element minOccurs="0" maxOccurs="1" name="receivedModules" type="msp:receivedModules" />
          <xs:element minOccurs="0" maxOccurs="1" name="receivedPremisesDisplays" type="msp:receivedPremisesDisplays" />
          <xs:element minOccurs="0" maxOccurs="1" name="receivedPropaneMeters" type="msp:receivedPropaneMeters" />
          <xs:element minOccurs="0" maxOccurs="1" name="transponderIDRange" type="msp:transponderIDRange">
            <xs:annotation>
              <xs:documentation>Range of idnetifiers for communications modules
								(transponders).</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="DERGroup">
    <xs:annotation>
      <xs:documentation>A grouping of Distributed Energy Resource (DER)
				facilities. The DERGroupName SHALL be carried in the
				DERGroup.primaryIdentifier.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="msp:mspObject">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="DERIDs" type="sandbox:DERIDs" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="DERIDs">
    <xs:annotation>
      <xs:documentation>Pointers to the distributed energy resources
				contained in this group.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="unbounded" name="DERID" type="sandbox:DERID" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DER">
    <xs:annotation>
      <xs:documentation>A distributed energy resource. The name of the DER
				SHALL be carried in the DER.primaryIdentifier.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="msp:mspPointObject">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" name="DERType" type="sandbox:DERType">
            <xs:annotation>
              <xs:documentation>The type of DER this plant represents.
							</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="contactInfo" type="com:contactInfo">
            <xs:annotation>
              <xs:documentation>Contact information for this DER.
							</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="serviceAgreements" type="msp:serviceAgreements">
            <xs:annotation>
              <xs:documentation>Interconnection and or rate agreement between
								this DER and others such as the interconnecting utility.
							</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="organizationAssociations" type="msp:organizationAssociations">
            <xs:annotation>
              <xs:documentation>Associations between this DER and
								organizations. One such association is the owner of the DER.
								Others might be operators, maintainers, etc.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="realPowerRatings" type="sandbox:realPowerRatings">
            <xs:annotation>
              <xs:documentation>Real power ratings for this DER.
							</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="reactivePowerRatings" type="sandbox:reactivePowerRatings" />
          <xs:element minOccurs="0" maxOccurs="1" name="apparentPowerRatings" type="sandbox:apparentPowerRatings">
            <xs:annotation>
              <xs:documentation>ApparentPowerRatings for DER.
							</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="storagePowerRating" type="com:realEnergy">
            <xs:annotation>
              <xs:documentation>The usable energy storage capability of the
								DER.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="notificationTime" type="com:duration">
            <xs:annotation>
              <xs:documentation>The notification time required before DER can
								start operation.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="DERRealPowerRampRate" type="sandbox:realPowerRampRate">
            <xs:annotation>
              <xs:documentation>The real power ramp rate for the DER.
							</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="generatingUnits" type="sandbox:generatingUnits" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="DERKind">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Unknown" />
      <xs:enumeration value="BatteryStorage" />
      <xs:enumeration value="Biogas" />
      <xs:enumeration value="Biomass" />
      <xs:enumeration value="Cogeneration" />
      <xs:enumeration value="CombinationDER" />
      <xs:enumeration value="CompressedAir" />
      <xs:enumeration value="Flywheel" />
      <xs:enumeration value="FuelCell" />
      <xs:enumeration value="Hydro" />
      <xs:enumeration value="Photovoltaic" />
      <xs:enumeration value="Thermal" />
      <xs:enumeration value="Wind" />
      <xs:enumeration value="Other" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="DERType">
    <xs:annotation>
      <xs:documentation>Type of DER that this data instance represents.
			</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="sandbox:DERKind">
        <xs:attribute name="otherKind" type="enum:otherKind" use="optional">
          <xs:annotation>
            <xs:documentation>If no acceptable hard enumeration value is
							available, the choice "Other" SHALL be used and the extended
							enumeration SHALL be included in this attribute.
						</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="realPowerRatings">
    <xs:complexContent mixed="false">
      <xs:extension base="com:mspExtensible">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="unbounded" name="realPowerRating" type="sandbox:realPowerRating" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="reactivePowerRatings">
    <xs:complexContent mixed="false">
      <xs:extension base="com:mspExtensible">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="unbounded" name="reactivePowerRating" type="sandbox:reactivePowerRating" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="inspection">
    <xs:complexContent mixed="false">
      <xs:extension base="msp:mspWork">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="elementStatus" type="xs:string">
            <xs:annotation>
              <xs:documentation>The status of the inspected item. For instance:
								Okay, Needs Attention, Needs Immediate Attention, Not
								Functional, or Okay as Left.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="elementCondition" type="xs:integer">
            <xs:annotation>
              <xs:documentation>Quality factor, expressed in percent. For
								instance 100 might been new, 0 might mean no life remaining.
							</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="serviceOrderID" type="com:objectID">
            <xs:annotation>
              <xs:documentation>A pointer to the service order this inspection
								was done under.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="observationList" type="sandbox:observationList" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="itemMaintained">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="1" name="itemIdentifier" type="com:objectRef" />
      <xs:element minOccurs="0" maxOccurs="1" name="maintenanceCode" type="xs:string" />
      <xs:element minOccurs="0" maxOccurs="1" name="conditionBefore" type="xs:string" />
      <xs:element minOccurs="0" maxOccurs="1" name="conditionAfter" type="xs:string" />
      <xs:element minOccurs="0" maxOccurs="1" name="remarks" type="xs:string" />
      <xs:element minOccurs="0" maxOccurs="1" name="measurements" type="msp:measurements" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="itemsMaintained">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="unbounded" name="itemMaintained" type="sandbox:itemMaintained" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="jobWorked">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="1" name="woNumber" type="xs:string">
        <xs:annotation>
          <xs:documentation>Work order number.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="jobNumber" type="xs:string">
        <xs:annotation>
          <xs:documentation>Job number.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="jobDescription" type="xs:string">
        <xs:annotation>
          <xs:documentation>Description of job.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="jobHours" type="xs:float">
        <xs:annotation>
          <xs:documentation>Hours worked on a specific job.
					</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="jobWorkedList">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="unbounded" name="jobWorked" type="sandbox:jobWorked">
        <xs:annotation>
          <xs:documentation>A list of all jobs worked.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="apparentPowerRatings">
    <xs:complexContent mixed="false">
      <xs:extension base="com:mspExtensible">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="unbounded" name="apparentPowerRating" type="sandbox:apparentPowerRating" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="realPowerRating">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="realPower" type="com:realPower" />
      <xs:element minOccurs="0" maxOccurs="1" name="description" type="xs:string">
        <xs:annotation>
          <xs:documentation>Description of rating type, such as
						DeliveredToGrid, ReceivedFromGrid, etc.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="reactivePowerRating">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="reactivePower" type="com:reactivePower" />
      <xs:element minOccurs="0" maxOccurs="1" name="description" type="xs:string">
        <xs:annotation>
          <xs:documentation>Description of rating type, such as
						DeliveredToGrid, ReceivedFromGrid, etc.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="apparentPowerRating">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="apparentPower" type="com:apparentPower" />
      <xs:element minOccurs="0" maxOccurs="1" name="description" type="xs:string">
        <xs:annotation>
          <xs:documentation>Description of rating type, such as
						DeliveredToGrid, ReceivedFromGrid, etc.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="realPowerRampRate">
    <xs:simpleContent>
      <xs:extension base="prim:floatType">
        <xs:attribute name="realPowerUnits" type="enum:realPowerUnits" use="optional">
          <xs:annotation>
            <xs:documentation>Units of real power associated with the ramp
							rate. For instance, if the ramp rate is 10 MW/hour, then the real
							power units are MW.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="timeUnits" type="enum:timeUnits" use="optional">
          <xs:annotation>
            <xs:documentation>The time units associated with the ramp rate.
							For instance, if the ramp rate is 10 MW/Hour, then the time units
							are hours.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="maintenanceDataSet">
    <xs:complexContent mixed="false">
      <xs:extension base="msp:mspObject">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="maintenanceDate" type="prim:mspDateTime" />
          <xs:element minOccurs="0" maxOccurs="1" name="description" type="xs:string" />
          <xs:element minOccurs="0" maxOccurs="1" name="itemsMaintained" type="sandbox:itemsMaintained" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="meterConditions">
    <xs:complexContent mixed="false">
      <xs:extension base="com:mspExtensible">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="kWhReading" type="xs:integer" />
          <xs:element minOccurs="0" maxOccurs="1" name="kWReading" type="xs:float" />
          <xs:element minOccurs="0" maxOccurs="1" name="kVArReading" type="xs:float" />
          <xs:element minOccurs="0" maxOccurs="1" name="powerFactorReading" type="xs:float" />
          <xs:element minOccurs="0" maxOccurs="1" name="resultHigh" type="xs:float" />
          <xs:element minOccurs="0" maxOccurs="1" name="resultLow" type="xs:float" />
