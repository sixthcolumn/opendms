  <xs:complexType name="requisitionResultItem">
    <xs:annotation>
      <xs:documentation>This is the status of the request for a specific
				material item that was made as part of a material requisition.
			</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="com:mspExtensible">
        <xs:sequence>
          <xs:element minOccurs="1" maxOccurs="1" name="materialItemID" type="com:objectID">
            <xs:annotation>
              <xs:documentation>The unique pointer to the materialItem. If
								provided, the materialID.primaryIdentifier field SHALL consist
								of the human-readable stockNumber for this materialItem.
							</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" maxOccurs="1" name="requisitionResult" type="sandbox:requisitionResult" />
          <xs:element minOccurs="0" maxOccurs="1" name="quantityOnHand" type="xs:float">
            <xs:annotation>
              <xs:documentation>If the material item requsitionResult is less
								than "Full", then this optional parameter may be used to give
								information on how many items of this type could be filled out
								of current, uncommitted stock.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="ETA" type="prim:mspDateTime">
            <xs:annotation>
              <xs:documentation>If the material item requisition result is less
								than "Full" then this optional field may be used to provide
								information about when new stock of this type might be
								available.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="requisitionResultKind">
